{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "VeryQueryable": {
    "AllowAnyQuery": true,
    "AllowAnyCORS": true,
    "Databases": [
      {
        "Name": "test",
        "Connection": "Data Source=test.db"
      }
    ],
    "DynamicPaths": [
      "/{db}/{table}",
      "/dynamic/{db}/{table}"
    ],
    "StaticPaths": [
      {
        "Database": "test",
        "Table": "text_codes",
        "Path": "/static/test/codes"
      },
      {
        "Database": "test",
        "Table": "text_type",
        "Path": "/static/test/type"
      }
    ]
  },
  "AllowedHosts": "*"
}
